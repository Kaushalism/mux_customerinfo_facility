    <script>
      function setCursorByID(cardbutton, pointer) {
        var elem;
        if (
          document.getElementById &&
          (elem = document.getElementById(cardbutton))
        ) {
          if (elem.style) elem.style.cursor = pointer;
        }
      }
    </script>
